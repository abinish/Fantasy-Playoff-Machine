<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    
	<script src="https://opensource.keycdn.com/angularjs/1.6.6/angular.min.js "></script>
	<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js "></script>
	<script src="https://cdnjs.cloudflare.com/ajax/libs/underscore.js/1.8.3/underscore.js"></script>
	
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.4.0/css/font-awesome.min.css">
	@Scripts.Render("~/bundles/scripts")
	@Scripts.Render("~/bundles/jquery")
    
    
    
    <title>Fantasy Football Hub</title>
   
    
    
	<script language="javascript">
	    //Global bag where MVC views can store pre-loaded data for the client
	    //Clients can then access this data using PreloadService.js
	    window.__preloadedData = {};
	</script>
     @Styles.Render("~/Content/css")
</head>
<body ng-app="fantasyPlayoffMachine" ng-cloak>
	<div class="navbar navbar-default navbar-fixed-top">
		<div class="container">
			<div class="navbar-header">
				@Html.ActionLink("Fantasy Football Hub", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })
				

			</div>
		</div>
	</div>
	<div class="container" style="width:95%">
		@RenderBody()
	</div>


</body>
</html>
